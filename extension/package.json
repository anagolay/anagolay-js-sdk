{
  "name": "@anagolay/extension",
  "version": "0.1.0",
  "description": "Send tips to verified creators, license content directly from verified creators, manage your accounts and much more.",
  "keywords": [
    "web3",
    "blockchain",
    "anagolay",
    "extension"
  ],
  "homepage": "https://gitlab.com/anagolay/anagolay-js/-/tree/main/extension#readme",
