  "type": "module",
  "typings": "./lib/extension.d.ts",
  "scripts": {
    "apiExtractor": "tsc -p tsconfig-api.json && api-extractor run --local",
    "build": "NODE_ENV=production svelte-kit sync && vite build --emptyOutDir",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "coverage": "vitest run --coverage",
    "crx": "crx pack ./dist --crx-version 3 -o anagolay-extension.crx",
    "dev": "vite",
    "preview": "vite preview",
    "test": "vitest",
    "watch": "NODE_ENV=development vite build --watch --mode development",
    "zip": "node makeZip.js"
  },
