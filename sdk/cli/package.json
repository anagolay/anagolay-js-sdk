  "license": "GPL3",
  "licenseFile": "./LICENSE",
  "keywords": [],
  "version": "0.7.0",
  "exports": "./lib/index.js",
  "types": "./lib/anagolay-cli.d.ts",
  "bin": {
    "anagolay": "./lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/anagolay/network-js-sdk.git",
    "directory": "sdk/cli"
  },
  "bugs": {
    "url": "https://gitlab.com/anagolay/network-js-sdk/issues"
  },
  "homepage": "https://gitlab.com/anagolay/network-js-sdk/-/tree/main/sdk/cli#readme",
  "scripts": {
    "build": "NODE_ENV=production heft test --clean",
    "watch": "env-cmd -f ../../.env heft build --clean --watch",
    "lint:fix": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "start": "node --experimental-modules --experimental-specifier-resolution=node lib/index.js",
    "link:bin:lib": "sudo ln -sf /workspace/sdk/cli/lib/index.js /usr/local/bin/anagolay && sudo chmod +x /usr/local/bin/anagolay",
    "link:bin": "sudo ln -sf /workspace/sdk/cli/dist/anagolay.js /usr/local/bin/anagolay && sudo chmod +x /usr/local/bin/anagolay"
  },
  "devDependencies": {
    "tslib": "~2.3.1",
    "typescript": "~4.5.5",
    "@rushstack/heft-node-rig": "~1.7.1",
    "@microsoft/api-extractor": "~7.19.4",
    "@rushstack/eslint-config": "~2.5.1",
    "@rushstack/heft": "~0.44.2",
