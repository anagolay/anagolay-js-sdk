  "scripts": {
    "build:bin": "pkg --targets node16-linux ./dist/anagolay.js --out-path binaries",
    "build": "NODE_ENV=production heft test --clean && pnpm build:bin",
    "watch:test": "heft test --clean --watch",
    "watch": "heft build --clean --watch",
    "lint:fix": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "start": "heft start",
    "link:bin:lib": "sudo ln -sf /workspace/anagolay-js/sdk/cli/lib/index.js /usr/local/bin/anagolay-lib && sudo chmod +x /usr/local/bin/anagolay-lib",
