{
  "name": "@anagolay/util",
  "version": "0.7.0",
  "author": "did:substrate:GHm6gp6khjcxQvnbnSsHdYejsFK1nNVxcGyGAsbDfsymtLr",
  "license": "GPL3",
  "licenseFile": "./LICENSE",
  "description": "Various utilities needed for Anagolay Network. They are carefully written to be fast and work in browser and nodejs env.",
  "module": "./lib/index.js",
  "main": "./lib_cjs/index.js",
  "types": "./lib/util.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "browser": "./lib/index.browser.js",
      "import": "./lib/index.js",
      "require": "./lib_cjs/index.js",
      "node": "./lib_cjs/index.js"
    },
    "./*": {
      "require": "./lib_cjs/*.js",
      "import": "./lib/*.js"
    },
    "./doh": {
      "types": "./lib/doh/doh.d.ts",
      "import": "./lib/doh/doh.js",
      "require": "./lib_cjs/doh/doh.js",
      "node": "./lib_cjs/doh/doh.js"
    }
  },
  "scripts": {
    "watch": "heft test --watch",
    "build": "heft build --clean && pnpm fix-lib",
    "fix-lib": "node ./config/add-js-to-esm.js"
  },
  "dependencies": {
    "@polkadot/util": "10.1.4",
    "ramda": "0.28.0",
    "@polkadot/api": "9.2.2",
    "@polkadot/types": "9.2.2",
    "@polkadot/types-codec": "9.2.2",
    "pino": "8.4.2"
  },
  "devDependencies": {
    "tslib": "2.4.0",
    "typescript": "4.7.4",
    "@rushstack/heft-web-rig": "0.11.3",
    "@microsoft/api-extractor": "7.29.2",
    "@rushstack/eslint-config": "3.0.0",
    "@rushstack/heft": "0.47.3",
    "@rushstack/heft-jest-plugin": "0.3.25",
    "@types/heft-jest": "1.0.3",
    "@types/node": "18.7.6",
    "@types/ramda": "0.27.64",
    "eslint": "8.22.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "env-cmd": "10.1.0",
    "nodemon": "2.0.15"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/anagolay/anagolay-js.git",
    "directory": "sdk/util"
  },
  "bugs": {
    "url": "https://gitlab.com/anagolay/anagolay-js/issues"
  },
  "homepage": "https://gitlab.com/anagolay/anagolay-js/-/tree/main/sdk/util#readme",
  "engines": {
    "node": ">=14.0.0"
  }
}
