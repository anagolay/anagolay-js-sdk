{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",

  "mainEntryPointFilePath": "<projectFolder>/lib/index.d.ts",
  "apiReport": {
    "enabled": true,
    "reportFolder": "../../../api-extractor-docs/md/"
  },
  "docModel": {
    "enabled": true,
    "apiJsonFilePath": "../../../api-extractor-docs/json/<unscopedPackageName>.api.json"
  },
  "dtsRollup": {
    "enabled": true,
    "betaTrimmedFilePath": "<projectFolder>/lib/<unscopedPackageName>.d.ts"
  }
}
