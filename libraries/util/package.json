{
  "name": "@anagolay/util",
  "version": "0.1.0",
  "description": "Various utilities needed for Anagolay Network. They are carefully written to be fast and work in browser and nodejs env.",
  "homepage": "https://gitlab.com/anagolay/anagolay-js/-/tree/main/sdk/util#readme",
  "bugs": {
    "url": "https://gitlab.com/anagolay/anagolay-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/anagolay/anagolay-js.git",
    "directory": "sdk/util"
  },
  "license": "GPL3",
  "author": "did:substrate:5Fn9SNUE8LihCm7Lq5dpPgBebGy5D7ZKWESDsWbdjsfV37d4",
  "type": "module",
  "exports": {
    ".": {
      "browser": "./lib/index.browser.js",
      "import": "./lib/index.js",
      "require": "./lib-commonjs/index.js",
      "node": "./lib-commonjs/index.js"
    },
    "./*": {
      "require": "./lib-commonjs/*.js",
      "import": "./lib/*.js"
    },
    "./doh": {
      "types": "./lib/doh/doh.d.ts",
      "import": "./lib/doh/doh.js",
      "require": "./lib-commonjs/doh/doh.js",
      "node": "./lib-commonjs/doh/doh.js"
    }
  },
  "main": "./lib-commonjs/index.js",
  "module": "./lib/index.js",
  "types": "./lib/util.d.ts",
  "scripts": {
    "_phase:build": "pnpm build",
    "_phase:code-quality": "eslint --ignore-path ../../.eslintignore .",
    "_phase:test": "heft test --no-build",
    "build": "heft build --clean",
    "test": "heft test --clean",
    "watch": "heft test --watch"
  },
  "dependencies": {
    "@polkadot/api": "9.6.1",
    "@polkadot/types": "9.6.1",
    "@polkadot/types-codec": "9.6.1",
    "@polkadot/util": "10.1.11",
    "pino": "8.4.2",
    "ramda": "0.28.0"
  },
  "devDependencies": {
    "@kelp_digital/heft-esm-transform-plugin": "workspace:*",
    "@kelp_digital/heft-library-rig": "workspace:*",
    "@microsoft/api-extractor": "7.29.2",
    "@rushstack/eslint-config": "3.0.0",
    "@rushstack/heft": "0.47.3",
    "@rushstack/heft-jest-plugin": "0.3.25",
    "@rushstack/heft-node-rig": "1.10.2",
    "@types/heft-jest": "1.0.3",
    "@types/node": "18.7.6",
    "@types/ramda": "0.27.64",
    "env-cmd": "10.1.0",
    "eslint": "8.22.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "nodemon": "2.0.15",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "licenseFile": "./LICENSE"
}
